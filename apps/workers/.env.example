# Environment
NODE_ENV=development

# Database
DATABASE_URL="postgresql://autograder:autograder_dev_password@localhost:5432/autograder_dev?schema=public"

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=autograder_redis_password

# Worker Configuration
CONCURRENT_JOBS=2
JOB_TIMEOUT_MS=600000
MAX_JOB_ATTEMPTS=3

# OpenAI API
OPENAI_API_KEY=sk-your-openai-api-key-here

# LangSmith
LANGSMITH_API_KEY=your-langsmith-api-key-here
LANGSMITH_PROJECT=autograder-dev

# AWS S3 (for artifacts)
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key
S3_BUCKET=autograder-artifacts

# Docker Execution
DOCKER_TIMEOUT_MS=600000
WORK_DIR=/tmp/autograder
